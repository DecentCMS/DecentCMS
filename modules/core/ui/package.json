{
  "name": "decent-core-ui",
  "version": "0.0.0",
  "description": "Core UI composition API for DecentCMS",
  "services": {
    "shape": {
      "path": "./lib/shape",
      "feature": "shape"
    },
    "placement-strategy": {
      "path": "./lib/file-placement-strategy",
      "feature": "file-placement-strategy",
      "dependencies": [
        "decent-core-file-resolution"
      ]
    },
    "rendering-strategy": {
      "path": "./lib/template-rendering-strategy",
      "feature": "template-rendering-strategy",
      "dependencies": ["decent-core-file-resolution"]
    },
    "view-engine": {
      "path": "./lib/markup-view-engine",
      "feature": "markup-view-engine",
      "dependencies": ["decent-core-tokens"]
    }
  },
  "features": {
    "shape": {
      "name": "Shape",
      "description": "Shape manipulation API for UI composition"
    },
    "file-placement-strategy": {
      "name": "File-Based Placement Strategy",
      "Description": "Dispatches shapes according to descriptions in files."
    },
    "template-rendering-strategy": {
      "name": "Template Rendering Strategy",
      "Description": "Renders shapes using template files."
    },
    "markup-view-engine": {
      "name": "Markup View Engine",
      "Description": "A view engine that uses Markup.js to render templates."
    }
  },
  "main": "./index.js",
  "scripts": {
    "test": "./test/index"
  },
  "author": "Bertrand Le Roy",
  "license": "MIT",
  "devDependencies": {
    "chai": "*",
    "mocha": "*",
    "proxyquire": "*"
  },
  "dependencies": {
    "decent-core-tokens": "0.0.0"
  }
}
